<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Index</title>
</head>
<body>
    <h1>Halaman Index</h1>
    <ul>
        <li><a href="/about">About</a></li>
        <li><a href="/contact">Contact</a></li>
    </ul>    

    <%# const nama = "Andika"; %> 
    <h1>Welcome <%= nama %> </h1>
    <h2>Contact List</h2>
    <!-- menampilkan data dari app -->
    <% if (cont.length > 1) { %>
        <% cont.forEach(cont => { %>
            <ul>
                <li>nama: <%= cont.nama %> </li>
                <li>email: <%= cont.email %> </li>
            </ul>
        <% }) %>
    <% } else { %>
        <p>data tidak ditemukan</p>
    <% } %> 
     
</body>
</html>